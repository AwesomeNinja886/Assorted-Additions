{
    "multipart": [
        {   "when": { "axis": "y", "top": "true", "bottom": "true" },
            "apply": { "model": "assorted_additions:block/chiseled_quartz_pillar_both" }
        },
        {   "when": { "axis": "x", "top": "true", "bottom": "true" },
            "apply": { "model": "assorted_additions:block/chiseled_quartz_pillar_both", "x": 90, "y": 90 }
        },
        {   "when": { "axis": "z", "top": "true", "bottom": "true" },
            "apply": { "model": "assorted_additions:block/chiseled_quartz_pillar_both", "x": 90 }
        },



        {   "when": { "axis": "y", "top": "true", "bottom": "false" },
            "apply": { "model": "assorted_additions:block/chiseled_quartz_pillar_top" }
        },
        {   "when": { "axis": "x", "top": "true", "bottom": "false" },
            "apply": { "model": "assorted_additions:block/chiseled_quartz_pillar_top", "x": 90, "y": 90 }
        },
        {   "when": { "axis": "z", "top": "true", "bottom": "false" },
            "apply": { "model": "assorted_additions:block/chiseled_quartz_pillar_top", "x": 90 }
        },



        {   "when": { "axis": "y", "top": "false", "bottom": "true" },
            "apply": { "model": "assorted_additions:block/chiseled_quartz_pillar_bottom" }
        },
        {   "when": { "axis": "x", "top": "false", "bottom": "true" },
            "apply": { "model": "assorted_additions:block/chiseled_quartz_pillar_bottom", "x": 90, "y": 90 }
        },
        {   "when": { "axis": "z", "top": "false", "bottom": "true" },
            "apply": { "model": "assorted_additions:block/chiseled_quartz_pillar_bottom", "x": 90 }
        },



        {   "when": { "axis": "y", "top": "false", "bottom": "false" },
            "apply": { "model": "assorted_additions:block/chiseled_quartz_pillar_middle" }
        },
        {   "when": { "axis": "x", "top": "false", "bottom": "false" },
            "apply": { "model": "assorted_additions:block/chiseled_quartz_pillar_middle", "x": 90, "y": 90 }
        },
        {   "when": { "axis": "z", "top": "false", "bottom": "false" },
            "apply": { "model": "assorted_additions:block/chiseled_quartz_pillar_middle", "x": 90 }
        }
    ]
}